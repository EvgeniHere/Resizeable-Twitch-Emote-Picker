{
  "manifest_version": 3,
  "name": "Bigger Twitch Emote Picker",
  "version": "0.1.0",
  "description": "",
  "permissions": ["tabs"],
  "host_permissions": ["https://www.twitch.tv/*"],
  "content_scripts": [
    {
      "matches": ["https://www.twitch.tv/*"],
      "js": ["contentScript.js"]
    }
  ],
  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  }
}
